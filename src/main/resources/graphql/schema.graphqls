type Query{
    listEmployees(page: Int, size: Int): [Employee]
}

type Mutation{
    addEmployee(employeeInfo: EmployeeInfo): [Employee]
}

input EmployeeInfo {
    name: String!
    addressLine1: String!
    addressLine2: String
    contact: String
    department: String!
    salary: Int
}

type Employee {
    employeeId: Int!
    name: String!
    addressLine1: String!
    addressLine2: String
    contact: String
    department: String!
    salary: Int
}
